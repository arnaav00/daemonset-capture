# API Settings
API_TITLE=API Security Capture Analyzer Service
API_VERSION=1.0.0
DEBUG=false
LOG_LEVEL=INFO

# Session Settings
SESSION_TTL_SECONDS=7200
MAX_CAPTURES_PER_BATCH=50
MAX_CAPTURES_PER_SESSION=1000

# Clustering Settings
SIMILARITY_THRESHOLD=0.70
MIN_CLUSTER_SIZE=2

# Similarity Weights (must sum to ~1.0)
WEIGHT_HEADER_SIGNATURE=0.35
WEIGHT_URL_BASE_PATH=0.25
WEIGHT_AUTH_SIGNATURE=0.20
WEIGHT_ERROR_SIGNATURE=0.12
WEIGHT_RESPONSE_SCHEMA=0.08

# Storage Settings (for production)
STORAGE_ENABLED=false
S3_BUCKET_NAME=your-bucket-name
S3_REGION=us-east-1
